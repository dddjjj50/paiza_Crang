//演習問題1
#include <stdio.h>
int main(void){
    // ここに計算式を書いて、出力する
    printf("%d",1234*5678/2);
}


//演習問題2
#include <stdio.h>
int main(void){
    int a = 31;
    int b = 17;
    // 以下に、aとbをかけ算し、結果を出力するコードを書いてください
    printf("%d",a*b);
}


//演習問題3
#include <stdio.h>
int main(void){
    int x = 8;
    int y = 5;
    // 以下に、xをyで割ったときの余りを計算し、結果を出力するコードを書いてください
    printf("%d",x%y);
}


//演習問題4
#include <stdio.h>
int main(void){
    // ここに計算式を書いて、出力する
    printf("%d",(1234+5678)*3);
}
