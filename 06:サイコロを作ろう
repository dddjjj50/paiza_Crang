//講義
// 数の表示とサイコロ
#include <stdio.h>
#include<stdlib.h>
#include<time.h>
int main(void){
    srand((unsigned int)time(NULL));
    int randnum=rand()%100+1;
    printf("スライムが%d匹あらわれた", randnum);
}


//演習問題
// 数の表示とサイコロ
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main(void){
    srand((unsigned int)time(NULL));
    int number = rand() % 6 + 1;
    printf("サイコロの目は%dです",number);
}

